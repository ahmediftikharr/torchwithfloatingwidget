<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="10dp"
                android:height="10dp"
                />
        </shape>
    </item>

    <item>
        <inset
            android:insetTop="0.2dp"
            android:insetLeft="0.2dp"
            android:insetRight="0.2dp"
            android:insetBottom="0.2dp">
            <shape android:shape="oval">
                <stroke
                    android:color="#121211"
                    android:width="2dp"/>
            </shape>
        </inset>
    </item>
</layer-list>

